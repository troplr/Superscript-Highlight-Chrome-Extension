{
  "manifest_version": 2,
  "name": "Find SuperScripts & Subscripts",
  "version": "1.6",
  "description": "Search and highlight SuperScripts",
  "browser_action": {
    "default_icon": "icon.png"
    /*"default_popup": "popup.html"*/
  },
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": [
        "style.css"
    ],
  "content_scripts": [ {
      "matches": [ "<all_urls>" ],
      "js": [ "jquery-3.3.1.min.js","jquery-ui.min.js"],
      "css":["style.css"]
   } ],
  "permissions": [
    "activeTab",  
  "tabs", "http://*/*", "https://*/*", "file:///C:/*"
  ]
}
